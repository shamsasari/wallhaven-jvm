<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
  <Appenders>
    <File name="MainLog" fileName="${sys:app.home}/main.log">
      <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %msg%n"/>
    </File>
  </Appenders>
  <Loggers>
    <Root level="info">
      <AppenderRef ref="MainLog"/>
    </Root>
  </Loggers>
</Configuration>
